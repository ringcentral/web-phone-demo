{
  "name": "untitled-app",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn lint && rm -rf .parcel-cache && parcel build src/index.html src/callback.html --dist-dir docs --public-url ./",
    "lint": "tsc --jsx react --skipLibCheck --noEmit --target ESNext --moduleResolution NodeNext --module NodeNext ./src/*.tsx && eslint --fix '**/*.{ts,tsx,js,jsx}' && prettier --write . && sort-package-json",
    "serve": "rm -rf .parcel-cache && parcel src/index.html src/callback.html --dist-dir temp --no-hmr"
  },
  "dependencies": {
    "@rc-ex/authorize-uri": "^1.1.16",
    "@rc-ex/core": "^1.4.0",
    "antd": "^5.20.3",
    "localforage": "^1.10.0",
    "manate": "^0.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ringcentral-web-phone": "^2.0.0-alpha.11"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.57.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "events": "^3.3.0",
    "parcel": "^2.12.0",
    "prettier": "^3.3.3",
    "process": "^0.11.10",
    "sort-package-json": "^2.10.1",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "tsx": "^4.19.0",
    "ttpt": "^0.12.0",
    "typescript": "^5.5.4",
    "vm-browserify": "^1.1.2",
    "yarn-upgrade-all": "^0.7.4"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "eslint"
    ]
  }
}
